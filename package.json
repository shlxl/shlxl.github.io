{
  "name": "xiaoling-home",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs --open",
    "docs:build": "node scripts/docs-build.mjs",
    "docs:preview": "vitepress preview docs",
    "new:post": "node scripts/new-post.mjs",
    "new:local": "node scripts/new-post-local.mjs",
    "post:local": "npm run new:local && npm run docs:build && npm run docs:preview",
    "post:promote": "node scripts/post-promote.mjs",
    "post:archive": "node scripts/post-archive.mjs",
    "post:remove": "node scripts/post-remove.mjs",
    "docs:aliases": "node scripts/gen-alias-redirects.mjs",
    "deploy:local": "node scripts/deploy-local.mjs",
    "lint": "biome lint",
    "format": "biome format",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@sugarat/theme": "*",
    "@biomejs/biome": "^1.8.3",
    "sass-embedded": "^1.92.1",
    "typescript": "^5.4.5",
    "vitepress": "^1.3.4"
  }
